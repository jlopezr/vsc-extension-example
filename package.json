{
  "name": "helloworld",
  "displayName": "HelloWorld",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "helloworld.countLines",
        "title": "Count lines"
      },
      {
        "command": "helloworld.countWords",
        "title": "Count words"
      },
      {
        "command": "extension.listOpenEditors",
        "title": "List open editors"
      },
      {
        "command": "extension.replaceJuanWithMola",
        "title": "Replace 'juan' with 'mola'"
      },
      {
        "command": "extension.listAllFiles",
        "title": "List All Files"
      },
      {
        "command": "helloworld.callWebService",
        "title": "Call Web Service"
      },
      {
        "command": "extension.createWindow",
        "title": "Create new window"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "axios": "^1.6.7",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
